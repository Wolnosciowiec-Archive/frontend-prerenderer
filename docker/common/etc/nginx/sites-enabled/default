server {
    listen 80 default_server;
    listen [::]:80 default_server;
    root /www/frontend-prerenderer;

    index index.html index.php;

    server_name _;

    location / {
        try_files $uri /index.php$is_args$args;
    }

    location ~ ^/index\.php(/|$) {
       include snippets/fastcgi-php.conf;
       fastcgi_pass localhost:9000;
    }

    location ~ /\.ht {
        deny all;
    }
}
